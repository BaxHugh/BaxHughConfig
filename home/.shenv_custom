
if [ -d ~/.bin ]; then
    export PATH="$HOME/.bin:${PATH}"
fi
if [ -d ~/.local/bin ]; then
    export PATH="$HOME/.local/bin:${PATH}"
fi

ZSH_PLUGINS=(git)
for file in ${ZSH_CUSTOM:=$HOME/.oh-my-zsh/custom}/plugins/*; do
    if [ $(basename $file) = "example" ]; then
        continue
    fi
    ZSH_PLUGINS+=($(basename $file))
done